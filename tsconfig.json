{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "./app-dev",
        "allowJs": true
    },
    "exclude": [
        "node_modules",
        "android",
        "ios",
        "app-dev",
        "app-prod"
    ]
}